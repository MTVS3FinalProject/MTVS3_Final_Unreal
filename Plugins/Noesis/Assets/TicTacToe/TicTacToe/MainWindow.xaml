              <Path x:Name="HexOuter" Data="{StaticResource TicTacToe.Geometry.HexOuter}">
                <Path.Fill>
                  <LinearGradientBrush EndPoint="0.884,1.011" StartPoint="0.219,0.14">
                    <GradientStop Color="#FFFDBD65"/>
                    <GradientStop Color="#00FDBD65" Offset="1"/>
                  </LinearGradientBrush>
                </Path.Fill>
              </Path>
              <Path x:Name="HexInner" Data="{StaticResource TicTacToe.Geometry.HexInner}">
                <Path.Fill>
                  <LinearGradientBrush EndPoint="0.246,0.143" StartPoint="0.749,0.903">
                    <GradientStop Color="#FFFDBD65"/>
                    <GradientStop Color="#00FDBD65" Offset="1"/>
                  </LinearGradientBrush>
                </Path.Fill>
              </Path>
            </Grid>
          </Viewbox>
          <Viewbox x:Name="HexagonBlue" Grid.Column="1" Grid.Row="1">
            <Grid x:Name="HexagonBlueGrid" RenderTransformOrigin="0.5,0.5" Effect="{StaticResource TicTacToe.Effect.HexaShadow}">
              <Grid.RenderTransform>
                <TransformGroup>
                  <ScaleTransform ScaleY="1.2" ScaleX="1.2"/>
                  <SkewTransform/>
                  <RotateTransform/>
                  <TranslateTransform/>
                </TransformGroup>
              </Grid.RenderTransform>
              <Path x:Name="HexBase1" Fill="{StaticResource TicTacToe.Brush.BlueGradient}" Data="{StaticResource TicTacToe.Geometry.HexBase}"/>
              <Path x:Name="HexShadow1" Fill="#FF008FC1" Data="{StaticResource TicTacToe.Geometry.HexShadow}"/>
              <Path x:Name="HexRim1" Fill="{StaticResource TicTacToe.Brush.BlueGradient}" Data="{StaticResource TicTacToe.Geometry.HexRim}"/>
              <Path x:Name="HexOuter1" Fill="{StaticResource TicTacToe.Brush.CyanGradient}" Data="{StaticResource TicTacToe.Geometry.HexOuter}"/>
              <Path x:Name="HexInner1" Fill="{StaticResource TicTacToe.Brush.CyanGradient2}" Data="{StaticResource TicTacToe.Geometry.HexInner}"/>
            </Grid>
          </Viewbox>
          <Viewbox x:Name="HexagonYellow" Grid.Column="0" Grid.Row="1">
            <Grid x:Name="HexagonYellowGrid" RenderTransformOrigin="0.5,0.5" Effect="{StaticResource TicTacToe.Effect.HexaShadow}">
              <Grid.RenderTransform>
                <TransformGroup>
                  <ScaleTransform ScaleX="1.2" ScaleY="1.2"/>
                  <SkewTransform/>
                  <RotateTransform/>
                  <TranslateTransform/>
                </TransformGroup>
              </Grid.RenderTransform>
              <Path x:Name="HexBase2" Data="{StaticResource TicTacToe.Geometry.HexBase}" Fill="{StaticResource TicTacToe.Brush.YellowGradient}"/>
              <Path x:Name="HexShadow2" Fill="#FFB76D23" Data="{StaticResource TicTacToe.Geometry.HexShadow}"/>
              <Path x:Name="HexRim2" Fill="{StaticResource TicTacToe.Brush.YellowGradient}" Data="{StaticResource TicTacToe.Geometry.HexRim}"/>
              <Path x:Name="HexOuter2" Fill="{StaticResource TicTacToe.Brush.YellowishGradient}" Data="{StaticResource TicTacToe.Geometry.HexOuter}"/>
              <Path x:Name="HexInner2" Fill="{StaticResource TicTacToe.Brush.YellowishGradient2}" Data="{StaticResource TicTacToe.Geometry.HexInner}"/>
            </Grid>
          </Viewbox>
          <TextBlock Text="PLAYER 1" FontSize="12" Grid.Row="0" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" Effect="{StaticResource TicTacToe.Effect.TextShadow}" Margin="0,0,0,6"/>
          <TextBlock Text="TIES" FontSize="12" Grid.Row="0" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" Effect="{StaticResource TicTacToe.Effect.TextShadow}"/>
          <TextBlock Text="PLAYER 2" FontSize="12" Grid.Row="0" Grid.Column="2" HorizontalAlignment="Center" VerticalAlignment="Center" Effect="{StaticResource TicTacToe.Effect.TextShadow}"/>
          <TextBlock x:Name="ScorePlayer1Fade" Text="{Binding ScorePlayer1}" Foreground="#00FFFFFF" Grid.Row="1" Grid.Column="0" FontSize="52" HorizontalAlignment="Center" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5">
            <TextBlock.RenderTransform>
              <TransformGroup>
                <ScaleTransform/>
                <SkewTransform/>
                <RotateTransform/>
                <TranslateTransform/>
              </TransformGroup>
            </TextBlock.RenderTransform>
          </TextBlock>
          <TextBlock x:Name="ScoreTiesFade" Text="{Binding ScoreTies}" Foreground="#00FFFFFF" Grid.Row="1" Grid.Column="1" FontSize="52" HorizontalAlignment="Center" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5">
            <TextBlock.RenderTransform>
              <TransformGroup>
                <ScaleTransform/>
                <SkewTransform/>
                <RotateTransform/>
                <TranslateTransform/>
              </TransformGroup>
            </TextBlock.RenderTransform>
          </TextBlock>
          <TextBlock x:Name="ScorePlayer2Fade" Text="{Binding ScorePlayer2}" Foreground="#00FFFFFF" Grid.Row="1" Grid.Column="2" FontSize="52" HorizontalAlignment="Center" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5">
            <TextBlock.RenderTransform>
              <TransformGroup>
                <ScaleTransform/>
                <SkewTransform/>
                <RotateTransform/>
                <TranslateTransform/>
              </TransformGroup>
            </TextBlock.RenderTransform>
          </TextBlock>
          <TextBlock x:Name="ScorePlayer1" Text="{Binding ScorePlayer1, FallbackValue=0}" Foreground="White" Grid.Row="1" Grid.Column="0" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5">
            <b:Interaction.Triggers>
              <b:DataTrigger Binding="{Binding Text, ElementName=ScorePlayer1}" Comparison="NotEqual" Value="0">
                <b:ControlStoryboardAction Storyboard="{StaticResource ScorePlayer1ChangedAnim}"/>
              </b:DataTrigger>
            </b:Interaction.Triggers>
            <TextBlock.RenderTransform>
              <TransformGroup>
                <ScaleTransform/>
                <SkewTransform/>
                <RotateTransform/>
                <TranslateTransform/>
              </TransformGroup>
            </TextBlock.RenderTransform>
          </TextBlock>
          <TextBlock x:Name="ScoreTies" Text="{Binding ScoreTies, FallbackValue=0}" Foreground="White" Grid.Row="1" Grid.Column="1" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5">
            <b:Interaction.Triggers>
              <b:DataTrigger Binding="{Binding Text, ElementName=ScoreTies}" Comparison="NotEqual" Value="0">
                <b:ControlStoryboardAction Storyboard="{StaticResource ScoreTiesChangedAnim}"/>
              </b:DataTrigger>
            </b:Interaction.Triggers>
            <TextBlock.RenderTransform>
              <TransformGroup>
                <ScaleTransform/>
                <SkewTransform/>
                <RotateTransform/>
                <TranslateTransform/>
              </TransformGroup>
            </TextBlock.RenderTransform>
          </TextBlock>
          <TextBlock x:Name="ScorePlayer2" Text="{Binding ScorePlayer2, FallbackValue=0}" Foreground="White" Grid.Row="1" Grid.Column="2" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5">
            <b:Interaction.Triggers>
              <b:DataTrigger Binding="{Binding Text, ElementName=ScorePlayer2}" Comparison="NotEqual" Value="0">
                <b:ControlStoryboardAction Storyboard="{StaticResource ScorePlayer2ChangedAnim}"/>
              </b:DataTrigger>
            </b:Interaction.Triggers>
            <TextBlock.RenderTransform>
              <TransformGroup>
                <ScaleTransform/>
                <SkewTransform/>
                <RotateTransform/>
                <TranslateTransform/>
              </TransformGroup>
            </TextBlock.RenderTransform>
          </TextBlock>
        </Grid>
      </Viewbox>
    </Grid>
  </Grid>
</UserControl>
